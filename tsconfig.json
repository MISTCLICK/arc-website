{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "verbatimModuleSyntax": true,
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ],
    "paths": {
      "@styles*": [
        "./src/styles/*"
      ],
      "@assets*": [
        "./src/assets/*"
      ],
      "@layouts*": [
        "./src/layouts/*"
      ],
      "@components*": [
        "./src/components/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}